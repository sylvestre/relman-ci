      <command>CLANG_VERSION=3.9
sudo /usr/bin/apt-get update
sudo /usr/bin/apt-get --reinstall install clang-${CLANG_VERSION} -y --force-yes
cd /usr/share/clang/scan-build-${CLANG_VERSION}/bin/
sudo /usr/bin/patch -p0 &lt; ~/patch/scan-build-show-desc.diff
</command>
    </hudson.tasks.Shell>
